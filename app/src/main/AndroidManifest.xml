<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="android.savin.android.helloworld">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ui.ntonsavin.fragment_activity.TmsFragmentActivity">
        </activity>
        <activity android:name=".ui.ntonsavin.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity"/>
        <activity android:name=".ui.ntonsavin.fragment_activity.TmsFragmentActivity" tools:ignore="DuplicateActivity"/>
    <service android:name=".ui.ntonsavin.TmsService"/>


    </application>

</manifest>